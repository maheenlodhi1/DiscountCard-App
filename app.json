{
  "expo": {
    "name": "DiscountCard",
    "slug": "DiscountCard",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.discountcard.app",
      "config": {
        "googleMapsApiKey": "AIzaSyAGtTzCFC5I7fdjV2E3QJTUMggoKSTYOv0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.discountcard.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAGtTzCFC5I7fdjV2E3QJTUMggoKSTYOv0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/custom_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/PlusJakartaSans-Medium.ttf",
            "./assets/fonts/PlusJakartaSans-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow this app to access your camera for scanning QR codes."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow this app to use your location to add offer details."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2a41c8f7-1c5e-4f0b-841b-0de6e132bba4"
      },
      "supportsRTL": true,
      "plugins": ["expo-localization"]
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/2a41c8f7-1c5e-4f0b-841b-0de6e132bba4"
    }
  }
}
